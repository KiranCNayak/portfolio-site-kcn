    "lint": "eslint --ignore-path .gitignore --ext .js .",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/react": "^2.3.0",
    "autoprefixer": "^10.4.15",
    "gatsby": "^5.12.1",
